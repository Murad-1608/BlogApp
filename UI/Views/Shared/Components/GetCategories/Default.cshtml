@using Entity.Concrete
@model List<Category>

<div class="tech-btm">
    <h4>Kateqoriyalar</h4>

    <ul class="list-group single">

        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                @item.Name
            </li>
        }
    </ul>
</div>