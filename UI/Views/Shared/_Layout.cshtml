<!DOCTYPE html>
<html lang="zxx">

<head>
    <title>Blog App</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta name="keywords" content="Weblog a Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />

    <script>
        addEventListener("load", function() {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <link href="~/Template/css/bootstrap.css" rel='stylesheet' type='text/css' />
    <link rel="stylesheet" href="~/Template/css/single.css">
    <link href="~/Template/css/style.css" rel='stylesheet' type='text/css' />
    <link href="~/Template/css/fontawesome-all.css" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800"
          rel="stylesheet">
          
          <script src="~/lib/jquery-validate/jquery.validate.js"></script>
          <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
</head>

<body>
    <!--Header-->

    <header>
        <div class="top-bar_sub_w3layouts container-fluid">
            <div class="row">
                <div class="col-md-4 logo text-left">
                    <a class="navbar-brand" asp-controller="Home" asp-action="Index">
                        <i class="fab fa-linode"></i> Muradov
                    </a>
                </div>

                @*LOGIN*@
                <partial name="~/PartialViews/UserLayout/LoginPartial.cshtml">


                    @*SOCIALMEADIA*@
                    <partial name="~/PartialViews/UserLayout/SocialMediaPartial.cshtml">

            </div>
        </div>

        @*NAVBAR*@
        <partial name="~/PartialViews/UserLayout/NavbarPartial.cshtml">

    </header>
    <!--//header-->
    <!--/banner-->
    <div class="banner-inner">
    </div>

    <!--//banner-->
    @RenderBody()


    <!--footer-->
    <partial name="~/PartialViews/UserLayout/FooterPartial.cshtml">

        <!---->
        <!-- js -->
        <script src="~/Template/js/jquery-2.2.3.min.js"></script>
        <!-- //js -->
        <!--/ start-smoth-scrolling -->
        <script src="~/Template/js/move-top.js"></script>
        <script src="~/Template/js/easing.js"></script>
        <script>
            jQuery(document).ready(function($) {
                $(".scroll").click(function(event) {
                    event.preventDefault();
                    $('html,body').animate({
                        scrollTop: $(this.hash).offset().top
                    }, 900);
                });
            });
        </script>
        <!--// end-smoth-scrolling -->

        <script>
            $(document).ready(function() {
                /*
                                        var defaults = {
                                                containerID: 'toTop', // fading element id
                                            containerHoverID: 'toTopHover', // fading element hover id
                                            scrollSpeed: 1200,
                                            easingType: 'linear'
                                            };
                                        */

                $().UItoTop({
                    easingType: 'easeOutQuart'
                });

            });
        </script>
        <a href="#home" class="scroll" id="toTop" style="display: block;">
            <span id="toTopHover" style="opacity: 1;"> </span>
        </a>

        <!-- //Custom-JavaScript-File-Links -->
        <script src="~/Template/js/bootstrap.js"></script>



</body>

</html>